{"vendor":{"plugins/ba-event":"1.0.0"},
"admin-config":{
"wpcf7":{
    "pages":{"admin.php":"page=wpcf7"},
	"forms":{
      "form-panel":{
       "fields":{
            "textarea-wpcf7-form":{"jquery":"textarea#wpcf7-form"}
            }
       }
  }
},
"post":{
	"pages":{"post.php":"", "post-new.php":"", "admin.php":"page=ba-event-settings"},
	"anchors":{"post-body-content":{"where":"first last"}},
	"forms":{
      "postbox":{
       "jquery":".postbox",
       "fields":{
            "postform":{"jquery":".postform option", "encode":"display"},
            "cmb2_select":{"jquery":".cmb2_select option", "encode":"display"},
            "cmb2-checkbox-list":{"jquery":".cmb2-checkbox-list label", "encode":"display"},
            "text-translatable":{"jquery":".q_translatable input"}
            }
       },
       "ba-event-options":{
       "fields":{
            "textarea-ba-event":{"jquery":"textarea"},
            "text-ba-event":{"jquery":"input.q_translatable"}
            }
       }
	}
},
"all_posts":{
	"pages":{"edit.php":""},
	"forms":{
      "table":{
       "jquery":"table.wp-list-table",
       "fields":{
            "td":{"jquery":"td", "encode":"display"}
            }
       },
      "wpbody-content":{
       "fields":{
            "h1":{"jquery":"h1", "encode":"display"}
            }
       }
	}
}
}
}